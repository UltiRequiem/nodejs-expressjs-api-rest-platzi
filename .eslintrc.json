{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:unicorn/recommended",
    "airbnb-base"
  ],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "error",
    "import-extensions": "off",
    "import/extensions": ["error", "always", { "ignorePackages": true }]
  }
}
